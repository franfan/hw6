function setup() { 
    createCanvas(800, 800);
    noStroke();
  } 
  function draw(){
    background(204);
    fill(0);
    rect(300,300,10,400);
    fill(255,0,0);
    arc(400, 300, 200, 200, PI, TWO_PI);
    fill(255,0,0);
    arc(200, 300, 200, 200, 0, PI);
    fill(255,0,0);
    arc(300, 200, 200, 200, 0.5*PI, 1.5*PI);
    fill(255,0,0);
    arc(300, 400, 200, 200, 1.5*PI, 0.5*PI);
  }
